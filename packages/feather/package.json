{
  "name": "@vue-icons/feather",
  "version": "1.0.19",
  "author": "@jooola",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-icons/vue-icons.git"
  },
  "homepage": "https://github.com/vue-icons/vue-icons/tree/master/packages/feather#readme",
  "bugs": {
    "url": "https://github.com/vue-icons/vue-icons/issues"
  },
  "files": [
    "dist/*",
    "lib/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/feather.esm.js",
      "require": "./dist/feather.umd.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "require": "./lib/*.js"
    }
  },
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepare": "yarn build",
    "prebuild": "ts-node -O '{\"module\": \"commonjs\"}' build.ts",
    "build": "rollup --config"
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/feather-icons": "^4.7.0",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.170",
    "@types/node": "^16.4.10",
    "@types/prettier": "^2.3.0",
    "feather-icons": "^4.28.0",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.3.2",
    "rollup": "^2.52.3",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "vue": "^3.1.1"
  }
}